{
  "apiVersion": "v1",
  "kind": "PersistentVolumeClaim",
  "metadata": {
      "name": "ebsc-1"
  },
  "spec": {
      "accessModes": [ "ReadWriteOnce" ],
      "resources": {
          "requests": {
              "storage": "1Gi"
          }
      },
      "selector":{
          "matchLabels": {
              "ebs-volume-type": "iops-ssd",
              "aws-availability-zone": "us-west-1"
          }
      }
  }
}

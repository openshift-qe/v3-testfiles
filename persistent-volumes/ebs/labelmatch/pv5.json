   "kind": "PersistentVolume",
   "metadata": {
      "name": "ebs-pv-5",
      "labels": {
        "ebs-volume-type": "gp-ssd",
        "aws-availability-zone": "us-west-1"
      }
   },
   "spec": {
      "capacity": {

{
  "apiVersion": "v1",
  "kind": "PersistentVolume",
  "metadata": {
    "name": "iscsi"
  },
  "spec": {
    "capacity": {
        "storage": "5Gi"
    },
    "accessModes": [ "ReadWriteMany" ],
    "iscsi": {
        "targetPortal": "192.168.0.225:3260",
        "iqn": "iqn.2016-04.test.com:storage.target00",
        "lun": 0,
        "fsType": "ext4",
        "readOnly": false
    },
    "persistentVolumeReclaimPolicy": "Recycle"
  }
}

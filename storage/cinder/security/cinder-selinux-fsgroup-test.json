{
  "kind": "Pod",
  "apiVersion": "v1",
  "metadata": {
    "name": "cinderpd",
    "creationTimestamp": null,
    "labels": {
      "name": "volume-test"
    }
  },
  "spec": {
    "containers": [
      {
        "name": "cinder",
        "image": "aosqe/storage",
        "ports": [
          {
            "containerPort": 8080,
            "protocol": "TCP"
          }
        ],
        "resources": {},

    "volumes": [
      {
          "name": "cinder",
          "cinder": {
              "volumeID": "614ac6ef-423d-490b-be4a-16fa829f20cc",
              "fsType": "ext4"
          }
      }
    ],
    "restartPolicy": "Always",
    "dnsPolicy": "ClusterFirst",
    "serviceAccount": ""
  },
  "status": {}
}

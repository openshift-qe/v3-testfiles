{
  "kind": "Pod",
  "apiVersion": "v1",
  "metadata": {
    "name": "hello-openshift",
    "creationTimestamp": null,
    "labels": {
      "name": "hello-openshift"
    }
  },
  "spec": {
    "containers": [
      {
        "name": "hello-openshift",
        "image": "aosqe/storage",
        "ports": [
          {
            "containerPort": 8080,
            "protocol": "TCP"
          }
        ],
        "resources": {},
        "volumeMounts": [
          {
            "name":"tmp",
            "mountPath":"/tmp"
          }
        ],
        "terminationMessagePath": "/dev/termination-log",
        "imagePullPolicy": "IfNotPresent",
        "securityContext": {
          "capabilities": {},
          "privileged": false
        }
      }
    ],
    "securityContext": {
         "supplementalGroups": [10000131],
         "runAsUser": 1001,
         "fsGroup": 4444,
         "seLinuxOptions": {
            "user": "system_u",
            "role": "object_r",
            "type": "svirt_sandbox_file_t",
            "level": "s0"
         }
    },
    "volumes": [
      {
        "name":"tmp",
        "emptyDir": {}
      }
    ],

    "restartPolicy": "Always",
    "dnsPolicy": "ClusterFirst",
    "serviceAccount": ""
  },
  "status": {}
}

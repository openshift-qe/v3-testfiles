{
    "kind": "Pod",
    "apiVersion": "v1",
    "metadata": {
        "labels": {
            "name": "hello-nginx-docker"
        },
        "name": "hello-nginx-docker"
    },
    "spec": {
        "containers": [
            {
                "image": "pweil/hello-nginx-docker",
                "name": "hello-nginx-docker-pod",
                "ports": [
                    {
                        "containerPort": 80
                    },
                    {
                        "containerPort": 443
                    }
                ]
            }
        ]
    }
}

{
  "apiVersion":"v1",
  "kind": "Pod",
  "metadata": {
    "name": "pod-with-more-than-remanent-memory",
    "labels": {
      "name": "resource-pod"
    }
  },
  "spec": {
    "containers": [{
      "name": "resource-pod",
      "image": "openshift/hello-openshift",
      "resources":{
        "limits":{
          "cpu":"10m",
          "memory":"<%=cb.pod_request_memory + 1%>"
        }
      }
    }]
  }
}
